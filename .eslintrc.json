{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "off",
        "max-len": "off",
        "no-console": "off",
        "no-param-reassign": "off",
        "func-names": "off",
        "no-plusplus": "off",
        "prefer-const": "off",
        "consistent-return": "off",
        "camelcase": "off",
        "indent": 2,
        "no-underscore-dangle": "off",
        "radix": "off",
        "no-restricted-syntax": "off",
        "no-unneeded-ternary": "off",
        "no-await-in-loop": "off",
        "eqeqeq": "off",
        "no-dupe-keys": "off",
        "default-param-last": "off"
    }
}
